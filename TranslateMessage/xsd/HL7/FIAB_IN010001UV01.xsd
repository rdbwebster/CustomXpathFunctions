			<xs:enumeration value="DST"/>
			<xs:enumeration value="ELOC"/>
			<xs:enumeration value="LOC"/>
			<xs:enumeration value="ORG"/>
			<xs:enumeration value="RML"/>
			<xs:enumeration value="VIA"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ParticipationVerifier">
